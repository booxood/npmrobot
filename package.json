{
  "name": "npmrobot",
  "version": "0.0.2",
  "description": "npm robot",
  "main": "index.js",
  "scripts": {
    "dev": "concurrent --kill-others \"npm run client-dev\" \"npm run api\"",
    "client-dev": "better-npm-run client-dev",
    "client-prod": "better-npm-run client-prod",
    "api": "better-npm-run api"
  },
  "betterScripts": {
    "client-dev": {
      "command": "node server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "client-prod": {
      "command": "webpack --config webpack/config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "api": {
      "command": "node api/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "keywords": [
    "npm",
    "robot"
  ],
  "engines": {
    "node": "5"
  },
  "author": "Liucw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/booxood/npmrobot.git"
  },
  "dependencies": {
    "co": "^4.4.0",
    "co-request": "^0.2.0",
    "copy-to": "^2.0.1",
    "koa": "^0.19.1",
    "koa-bodyparser": "^1.4.0",
    "koa-cors": "0.0.14",
    "koa-error": "^1.1.3",
    "koa-generic-session": "^1.6.0",
    "koa-generic-session-mongo": "^0.1.1",
    "koa-hbs": "^0.7.0",
    "koa-logger": "^1.2.2",
    "koa-router": "^4.2.0",
    "koa-static": "^1.4.9",
    "later": "^1.1.6",
    "moment": "^2.9.0",
    "mongoose": "^4.0.1",
    "newrelic": "^1.17.0",
    "node-uuid": "^1.4.2",
    "nodemailer": "^1.3.1",
    "require-by-env": "1.0.0",
    "express": "^4.13.3",
    "history": "^1.13.1",
    "http-proxy": "^1.12.0",
    "isomorphic-fetch": "^2.2.0",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "better-npm-run": "0.0.5",
    "concurrently": "1.0.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "istanbul": "*",
    "mocha": "*",
    "node-sass": "^3.4.2",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "sass-loader": "^3.1.2",
    "should": "*",
    "style-loader": "^0.12.4",
    "supertest": "*",
    "travis-cov": "*",
    "webpack": "^1.12.8",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
